<?xml version='1.0' encoding='utf-8'?>
<plugin id="elastos-essentials-plugins-appmanager" version="1.0.0"
    xmlns="http://apache.org/cordova/ns/plugins/1.0"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <name>AppManager</name>
    <js-module name="AppManager" src="www/AppManager.js">
        <clobbers target="appManager" />
    </js-module>
    <platform name="android">
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="AppManager">
                <param name="android-package" value="org.elastos.essentials.plugins.appmanager.AppManagerPlugin" />
                <param name="onload" value="true" />
            </feature>
        </config-file>
         <source-file src="src/android/AppManagerPlugin.java" target-dir="src/org/elastos/essentials/plugins/appmanager" />
    </platform>
    <!-- <platform name="ios">
        <config-file parent="/*" target="config.xml">
            <feature name="AppManager">
                <param name="ios-package" value="AppManagerPlugin" />
                <param name="onload" value="true" />
            </feature>
        </config-file>
        <source-file src="src/ios/AppManagerPlugin.swift" />
    </platform> -->

</plugin>
